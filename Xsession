HOSTNAME=`hostname`

if [ "$HOSTNAME" = "jigglypuff" ]; then
  xrandr --output DVI-0 --primary --mode 1920x1200 --rotate normal --left-of DisplayPort-0 --mode 1920x1200 --rotate normal
fi

xrdb -merge .Xresources

feh --bg-fill ~/dotfiles/wallpaper.jpg

urxvtd -q -f -o
export TERMINAL=urxvtc

#exec /usr/bin/xmonad
exec i3

killall urxvtd
